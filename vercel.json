{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "/src/style.css",
            "dest": "/public/src/style.css"
        },
        {
            "src": "/src/script.js",
            "dest": "/public/src/script.js"
        },
        {
            "src": "/src/handler.php",
            "dest": "/public/src/handler.php"
        },
        {
            "src": "/img/(.*).png",
            "dest": "/public/img/$1"
        },
        {
            "src": "/img/(.*).svg",
            "dest": "/public/img/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}